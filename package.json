  },
  "homepage": "https://github.com/beverloo/peter.sh#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/beverloo/peter.sh.git"
  },
  "license": "MIT",
  "author": "Peter Beverloo",
  "dependencies": {
    "next": "^15.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/node": "24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "eslint": "9.30.1",
    "eslint-config-next": "15.3.4",
    "eslint-plugin-unused-imports": "^4.1.4",
    "typescript": "5.8.3"
  }
}
